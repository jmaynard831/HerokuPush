<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ProductRetrieval</servlet-name>
  	<display-name>ProductRetrieval</display-name>
  	<description></description>
  	<servlet-class>com.servlet.ProductRetrieval</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddProduct</servlet-name>
  	<display-name>AddProduct</display-name>
  	<description></description>
  	<servlet-class>com.servlet.AddProduct</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProductRetrieval</servlet-name>
  	<url-pattern>/ProductRetrieval</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddProduct</servlet-name>
  	<url-pattern>/AddProduct</url-pattern>
  </servlet-mapping>
</web-app>
